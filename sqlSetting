CREATE TABLE VOCAT(
        VONUM  NUMBER PRIMARY KEY,
        VODIRNUM NUMBER,
        VOTBNAME NVARCHAR2(50),
        VOSP NVARCHAR2(50),
        VOHW NVARCHAR2(50),
        VOIMG NVARCHAR2(2000),
        VODATE NVARCHAR2(50),
        VOIMGLV NUMBER
);

CREATE TABLE VOCADIR(
    VODIRNUM  NUMBER PRIMARY KEY,
    VOTBNAME NVARCHAR2(50),
    VODATE NVARCHAR2(50)
);

CREATE SEQUENCE VOC_SEQ INCREMENT BY 1;
CREATE SEQUENCE VOD_SEQ INCREMENT BY 1;
